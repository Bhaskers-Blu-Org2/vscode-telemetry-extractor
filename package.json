{
  "name": "vscode-telemetry-extractor",
  "version": "0.0.5",
  "description": "Extracts telemetry from VS Code",
  "main": "out/extractor.js",
  "scripts": {
    "test": "mocha out/tests/mocha/*.js",
    "setup-repos": "./setup-repositories.sh",
    "update-repos": "./update-repositories.sh",
    "clean": "rm -f ./declarations-extensions-resolved.json && rm -f ./declarations-resolved.json",
    "clean-all": "rm -rf ./src/telemetry-sources && rm -f ./declarations-extensions-resolved.json && rm -f ./declarations-resolved.json",
    "extract-core": "node ./out/cli-extract.js --sourceDir ./src/telemetry-sources/vscode --excludedDirPattern extensions  --outputDir . --applyEndpoints --includeIsMeasurement --patchWebsiteEvents",
    "extract-extensions": "node ./out/cli-extract-extensions.js --sourceDir ./src/telemetry-sources/ --outputDir . --applyEndpoints --includeIsMeasurement",
    "extract-all": "npm run-script extract-core && npm run-script extract-extensions",
    "merge": "rm -f ./merger/merged-telemetry.json && node ./out/merge.js"
  },
  "author": "lramos15",
  "license": "MIT",
  "dependencies": {
    "command-line-args": "^5.1.1",
    "ts-morph": "^2.3.0",
    "vscode-ripgrep": "^1.2.5"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "mocha": "^6.1.4"
  }
}