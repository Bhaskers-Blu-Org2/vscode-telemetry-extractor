{
  "name": "vscode-telemetry-extractor",
  "version": "1.2.1",
  "description": "Extracts telemetry from VS Code",
  "main": "out/index.js",
  "typings": "vscode-telemetry-extractor.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode-telemetry-extractor"
  },
  "scripts": {
    "test": "mocha out/tests/mocha/*.js",
    "clean": "rm -f ./declarations-extensions-resolved.json && rm -f ./declarations-resolved.json",
    "clean-all": "rm -rf ./src/telemetry-sources && rm -f ./declarations-extensions-resolved.json && rm -f ./declarations-resolved.json",
    "extract-core": "node ./out/extractor.js --sourceDir ./src/telemetry-sources/vscode --excludedDirPattern extensions --eventPrefix monacoworkbench/  --outputDir . --applyEndpoints --includeIsMeasurement",
    "merge": "rm -f ./merger/merged-telemetry.json && node ./out/merge.js",
    "compile": "tsc"
  },
  "bin": {
    "vscode-telemetry-extractor": "./out/extractor.js"
  },
  "author": "lramos15",
  "license": "MIT",
  "dependencies": {
    "command-line-args": "^5.1.1",
    "ts-morph": "^2.3.0",
    "vscode-ripgrep": "^1.3.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "mocha": "^6.1.4",
    "typescript": "^3.5.2"
  }
}
